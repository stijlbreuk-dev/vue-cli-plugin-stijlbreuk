{
	"useConfigFiles": true,
	"plugins": {
		"@vue/cli-plugin-babel": {},
		"@vue/cli-plugin-typescript": {
			"classComponent": false,
			"useTsWithBabel": true
		},
		"@vue/cli-plugin-pwa": {},
		"@vue/cli-plugin-router": {
			"historyMode": true
		},
		"@vue/cli-plugin-vuex": {},
		"@vue/cli-plugin-eslint": {
			"config": "prettier",
			"lintOn": ["save", "commit"]
		},
		"@vue/cli-plugin-unit-jest": {},
		"@vue/cli-plugin-e2e-cypress": {}
	},
	"vueVersion": "3",
	"cssPreprocessor": "dart-sass"
}
